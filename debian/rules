#!/usr/bin/make -f

#export DH_VERBOSE=1

%:
	dh $@ --with python2

override_dh_clean:
	dh_clean
	rm -rf build dist MANIFEST

override_dh_install:
	dh_install
	install -m 0644 bash_completion.d/xcapclient debian/python-xcaplib/etc/bash_completion.d/xcapclient

.PHONY: override_dh_clean override_dh_install
